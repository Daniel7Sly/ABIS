@main[]->{
	
    PrintL: @Factorial[5];
    
    Set: num|i; Eql: $i | 5;
    Set: num|j; Eql: $j | 1000;

    PrintL: @SumOfSquares[$i, $j];

    If: (($i + $j) > (@SumOfSquares[$i, $j] - 1000))| flag;

    End::; Return:;

    flag:
        PrintL:YAY;
}

@Factorial[num:num]->num{
    Set:num|i;
    Eql:$i|1;
    Set:num|rslt;
    Eql:$rslt|1;

    Loop:
        Eql:$rslt|($rslt * $i);
    LoopEnd:
        Eql:$i|($i + 1);
        If:($i < ($num + 1))|Loop;

    Return: $rslt;
}

@SumOfSquares[num:a;num:b] -> num {
	Set: num|rslt;
	Eql: $rslt|(($a * $a) + ($b * $b));
	Return:$rslt;
}